{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "import/extensions": [0, {
            "js": "always",
            "json": "never"
        }
        ],
        "no-plusplus": 0
    }
}
